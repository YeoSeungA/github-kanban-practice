//tsconfig.json
//compilerOptions 내의 속성은 자유롭게 커스텀 할 수 있습니다.
{
    "compilerOptions": {
      // "target": "es6",
      // "module": "commonjs",
      "sourceMap": true,
      "outDir": "./dist",
      "jsx": "react-jsx", // 또는 "react"
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext"
    },
    "include": [
      "src/**/*"
    ]
  }